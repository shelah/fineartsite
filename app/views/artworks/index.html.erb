<h1>Listing artworks</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Width</th>
    <th>Height</th>
    <th>Img width</th>
    <th>Img height</th>
    <th>Year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artworks.each do |artwork| %>
  <tr>
    <td><%= artwork.title %></td>
    <td><%= artwork.width %></td>
    <td><%= artwork.height %></td>
    <td><%= artwork.img_width %></td>
    <td><%= artwork.img_height %></td>
    <td><%= artwork.year %></td>
    <td><%= link_to 'Show', artwork %></td>
    <td><%= link_to 'Edit', edit_artwork_path(artwork) %></td>
    <td><%= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Artwork', new_artwork_path %>
