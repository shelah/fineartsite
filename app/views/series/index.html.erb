<% if current_user %>
    <div class='adminlinks'>
      <%= link_to 'New Series', new_series_url() %>
    </div>
<% end %>

<div id='container'>
  <div id="container-content">
    <div id="sidebar">
      <ul>
        <% @series_plural.reverse.each do |series| %>
            <li><%=h link_to series.name, :id => series.id, :action => 'show' %></li>
        <% end %>
      </ul>
    </div>
    <div id="main">
      <div id="main-painting">
      </div>
    </div>
  </div>
</div>
