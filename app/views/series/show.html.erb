<p>
  <b>Name:</b>
  <%=h @series.name %>
</p>

<p>
  <b>Begin date:</b>
  <%=h @series.begin_date %>
</p>

<p>
  <b>End date:</b>
  <%=h @series.end_date %>
</p>

<table>
  <tbody>
    <% @series.artworks.each do |artwork| %>
        <tr>
          <td><%=h artwork.title %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_series_path(@series) %> |
<%= link_to 'Back', :controller => 'series', :action => 'index' %>